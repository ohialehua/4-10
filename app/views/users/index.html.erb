  <%= render partial: 'sidemenu' %>
    <div class="col-md-8">
      <h2>Users</h2>
      <table class="table">
        <thead>
          <th>image</th>
          <th>name</th>
          <th></th>
        </thead>
        <tbody>
          <% @users.each do |user| %>
          <td><%= attachment_image_tag @user, :profile_image, size: "40x40", fallback: "no_image.jpg"%></td>
          <td><%= user.name %></td>
          <td><%= link_to 'show', user_path(user.id) %></td>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>